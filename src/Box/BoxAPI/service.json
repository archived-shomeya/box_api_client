{
  "name": "Box.com",
  "apiVersion": "2.0",
  "baseUrl": "https://api.box.com/2.0/",
  "description": "Box.com is a cloud file storage service",
  "operations": {
    "GetFolder": {
      "httpMethod": "GET",
      "uri": "folders/{id}",
      "parameters": {
        "id": {
          "location": "uri",
          "description": "Folder to retrieve by ID",
          "type": "integer",
          "required": true
        }
      }
    },
    "CreateFolder": {
      "httpMethod": "POST",
      "uri": "folders",
      "parameters": {
        "name": {
          "location": "json",
          "type": "string",
          "required": true
        },
        "parent": {
          "location": "json",
          "type": "object",
          "properties": {
            "id": {
              "location": "json",
              "type": "integer",
              "required": true
            }
          }
        }
      }
    }
  }
}
